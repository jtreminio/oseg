{%- macro print_security(printable) -%}
    {% set comment = "" %}
    {% if not printable.is_primary %}{% set comment = "// " %}{% endif %}

    {%- if printable.method == "api_key" %}{{ comment }}config.ApiKey = new Dictionary<string, string> {["{{ printable.name }}"] = "{{ printable.value }}"};
    {%- elif printable.method == "username" %}{{ comment }}config.Username = "{{ printable.value }}";
    {%- elif printable.method == "password" %}{{ comment }}config.Password = "{{ printable.value }}";
    {%- elif printable.method == "access_token" %}{{ comment }}config.AccessToken = "{{ printable.value }}";
    {%- endif %}
{%-endmacro -%}

