{%- macro print_scalar(printable) -%}
{{ printable.value }}
{%- endmacro -%}

{%- macro print_scalar_array(printable) -%}
    {% if printable.value == None %}nil{% else %}
[
        {% for i in printable.value %}
    {{ i }},
        {% endfor %}
]
    {%- endif %}
{%- endmacro -%}

{%- macro print_file(printable) -%}
    {% if printable.value == None %}nil{% else %}
File.new("{{ printable.value }}", "r")
    {%- endif %}
{%- endmacro -%}

{%- macro print_file_array(printable) -%}
    {% if printable.value == None %}nil{% else %}
[
        {% for i in printable.value %}
    File.new("{{ i }}", "r"),
        {% endfor %}
]
    {%- endif %}
{%- endmacro -%}

{%- macro print_free_form(printable) -%}
    {% if printable.value == None %}nil{% else %}
{
        {% for k, v in printable.value.items() %}
    "{{ k }}": {{ v }},
        {% endfor %}
}
    {%- endif %}
{%- endmacro -%}

{%- macro print_free_form_array(printable) -%}
    {% if printable.value == None %}nil{% else %}
[
        {% for obj in printable.value %}
    {
            {% for k, v in obj.items() %}
        "{{ k }}": {{ v }},
            {% endfor %}
    },
        {% endfor %}
]
    {%- endif %}
{%- endmacro -%}

{%- macro print_object(printable) -%}
{{ printable.value }}
{%- endmacro -%}

{%- macro print_object_array(printable) -%}
[
    {% for i in printable.value %}
    {{ i|snake_case }},
    {% endfor %}
]
{%- endmacro -%}

{%- import "_macros.jinja2" as m with context -%}

require "{{ config.gem_name }}"

{{ config.module_name }}.configure do |config|
end

{% for obj_name, obj in property_container.flattened_objects().items() %}
    {% set parent_name = obj_name if obj_name != property_container.body.type else "" %}
    {% if not obj.is_array %}
{{ obj_name|print_variable }} = {{ config.module_name }}::{{ obj.type|split_uc|pascal_case }}.new
        {% call(properties) m.parse_object_properties_macro(property_container, obj, 0) %}
            {% for name, value in properties.items() %}
{{ obj_name|print_variable }}.{{ name|print_variable }} = {{ value }}
            {% endfor %}
        {% endcall %}
    {% else %}
{{ obj_name|print_variable }} = {{ "" }}
        {%- call(value) m.parse_object_list_properties_macro(obj, 0) %}
    {{- value }}
        {% endcall %}
    {% endif %}

{% endfor %}
begin
    {{ "" -}}
    {% if operation.response and operation.response.body  %}response = {% endif -%}
    {{ config.module_name }}::{{ operation.api_name|split_uc|pascal_case }}Api.new.{{ operation.operation_id|snake_case }}{%- if not property_container.has_data %}

{% else %}(
{% call(properties) m.parse_api_call_properties_macro(property_container, 8, True) %}
    {% for name, value in properties.items() %}
        {{ value }},
    {% endfor %}
{% endcall %}
{% call(properties) m.parse_api_call_properties_macro(property_container, 8, False) %}
    {% if properties %}
        {
        {% for name, value in properties.items() %}
            {{ name|print_variable }}: {{ value }},
        {% endfor %}
        },
    {% endif %}
{% endcall %}
    )
{% endif %}
{% if operation.response and operation.response.body and not operation.response.is_binary %}

    p response
{% elif operation.response and operation.response.is_binary %}

    FileUtils.cp(response.path, "path/to/file.zip")
{% endif %}
rescue {{ config.module_name }}::ApiError => e
    puts "Exception when calling {{ operation.api_name|pascal_case }}#{{ operation.operation_id|snake_case }}: #{e}"
end
{{ "" }}
